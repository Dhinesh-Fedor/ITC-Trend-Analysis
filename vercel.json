{
  "version": 2,
  "builds": [
    { "src": "api/index.py", "use": "@vercel/python", "config": { "runtime": "python3.9", "maxLambdaSize": "50mb" } }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/index.py" },
    { "src": "/stockdata/(.*)", "dest": "/api/index.py" },
    { "src": "/predict", "methods": ["POST"], "dest": "/api/index.py" },
    { "src": "/status", "dest": "/api/index.py" },
    { "src": "/(.*)", "dest": "/api/index.py" }
  ]
}
